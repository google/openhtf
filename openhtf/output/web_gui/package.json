{
  "name": "openhtf-frontend",
  "version": "2.0.0",
  "scripts": {
    "build": "rimraf dist && webpack --progress --profile --bail",
    "ci": "npm run e2e && npm run test",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "lint": "tslint --force \"src/**/*.ts\"",
    "postinstall": "npm run webdriver-update",
    "posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "pretest": "npm run lint",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "start": "npm run server",
    "start:hmr": "npm run server -- --hot",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "watch": "webpack --watch --progress --profile",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@types/rx": "^4.1.1",
    "core-js": "^2.5.5",
    "node-normalize-scss": "^3.0.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.5.10",
    "sockjs-client": "^1.1.4",
    "tslib": "^1.7.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angularclass/hmr": "^1.0.1",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^6.0.106",
    "@types/selenium-webdriver": "2.53.39",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.3.2",
    "awesome-typescript-loader": "^3.5.0",
    "codelyzer": "2.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^3.2.0",
    "json-loader": "^0.5.3",
    "karma": "6.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-remap-istanbul": "0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.2",
    "node-sass": "^7.0.3",
    "null-loader": "0.1.1",
    "postcss-loader": "^1.1.0",
    "protractor": "^4.0.10",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.7",
    "shelljs": "^0.8.5",
    "style-loader": "^0.13.0",
    "terser-webpack-plugin-legacy": "^1.2.5",
    "ts-helpers": "^1.1.1",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.5.1",
    "typescript": "^2.8.1",
    "url-loader": "^0.5.6",
    "webpack": "^3.12.0",
    "webpack-dev-server": "2.3.0"
  }
}
